<Project>
  <!-- Import parent Directory.Build.props -->
  <Import Project="$([MSBuild]::GetPathOfFileAbove('Directory.Build.props', '$(MSBuildThisFileDirectory)../'))" />
  
  <PropertyGroup>
    <!-- Suppress localization warnings for test projects -->
    <NoWarn>$(NoWarn);CA1303;CA2007;CA1031</NoWarn>
  </PropertyGroup>
</Project>